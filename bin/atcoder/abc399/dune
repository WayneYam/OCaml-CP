
 (rule
 (target A_submit.ml)
 (deps A.ml)
 (action
  (with-stdout-to %{target}
   (with-stdin-from %{deps}
    (chdir %{project_root} (run ./expander))))))

(executable
 (name A_submit)
 (modules A_submit)
 (libraries base core stdio str zarith)
 (preprocess (pps ppx_jane))
 (flags (-dsource)))

(executable
 (name A)
 (modules A)
 (libraries lib base core stdio str zarith)
 (preprocess (pps ppx_jane)))


